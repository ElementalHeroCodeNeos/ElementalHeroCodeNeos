#include<bits/stdc++.h>

using namespace std;

bool check(string s){
	stack<int> st;
	for(char x : s){
		if(x == '(' || x == '[' || x == '{'){
			st.push(x);
		}
		else{
			if(st.empty()) return false;
			char y = st.top(); st.pop();
			if((x == ')' && y == '(') || (x == ']' && y == '[') || (x == '}' && y == '{'))
				continue;
			return false;
		}
	}
	return st.empty();
}

int main(){
	int tc; cin >> tc;
	while(tc--){
		string s; cin >> s;
		if(check(s)) cout << "YES\n";
		else cout << "NO\n";
	}
	return 0;
}
