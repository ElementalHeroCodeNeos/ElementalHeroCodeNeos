#include<bits/stdc++.h>

using namespace std;

int BFS(int s, int t){
	queue<pair<int, int>> q;
	q.push({s, 0});
	set<int> se;
	se.insert(s);
	while(!q.empty()){
		auto x = q.front(); q.pop();
		if(x.first == t){
			return x.second;
		}
		if(x.first > 1 && !se.count(x.first - 1)){
			q.push({x.first - 1, x.second + 1});
			se.insert(x.first - 1);
		}
		if(x.first < t && !se.count(x.first * 2)){
			q.push({x.first * 2, x.second + 1});
			se.insert(x.first * 2);
		}
	}
}		

int main(){
	int tc; cin >> tc;
	while(tc--){
		int s, t; cin >> s >> t;
		cout << BFS(s, t) << endl;
	}
	return 0;
}
