#include <bits/stdc++.h>

using namespace std;

int so(string s, char x, char y){
	for(int i=0; i<s.size(); i++){
		if(s[i] == x){
			s[i] = y;
		}
	}
	return stoi(s);
}

int main(){
	string a, b; cin >> a >> b;
	cout << so(a, '6', '5') + so(b, '6', '5') << " " << so(a, '5', '6') + so(b, '5', '6');
	return 0;
}
	
