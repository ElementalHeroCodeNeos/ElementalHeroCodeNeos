#include <bits/stdc++.h>

using namespace std;

int n, k, a[15], S, dem = 0;

void Try(int start, int sum, int cnt){
	if(cnt == k && start == n + 1){
		dem++;
		return;
	}
	for(int end = start; end <= n; end++){
		sum += a[end];
		if(sum == S){
			Try(end + 1, 0, cnt + 1);
		}		
	}
}

int main(){
	cin >> n >> k;
	S = 0;
	for(int i=1; i<=n; i++){
		cin >> a[i];
		S += a[i];
	}
	S /= k;
	Try(1, 0, 0);
	cout << dem << endl;
	return 0;
}

