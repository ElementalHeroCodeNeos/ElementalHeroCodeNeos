#include <bits/stdc++.h>

using namespace std;

using ll = long long;

vector<ll> v;
void init(){
	queue<ll> q;
	q.push(9);
	while(1){
		ll top = q.front(); q.pop();
		if(top > 1e18) break;
		v.push_back(top);
		q.push(top * 10);
		q.push(top * 10 + 9);
	}
}

int main(){
	init();
	int tc; cin >> tc;
	while(tc--){
		int n; cin >> n;
		for(ll x : v){
			if(x % n == 0){
				cout << x << endl;
				break;
			}
		}
	}
	return 0;
}
