#include <bits/stdc++.h>

using namespace std;

int n, k, a[12], x[12];
int sum;
bool ok;

void Try(int i, int pos){
	for(int j=pos; j<=n; j++){
		x[i] = a[j];
		sum += a[j];
		if(sum == k){
			ok = 1;
			cout << "[";
			for(int l=1; l<=i-1; l++){
				cout << x[l] << " ";
			}
			cout << x[i] << "] ";
		}
		else Try(i + 1, j + 1);
		sum -= a[j];
	}
}

int main(){
	int tc; cin >> tc;
	while(tc--){
		ok = 0;
		cin >> n >> k;
		for(int i=1; i<=n; i++){
			cin >> a[i];
		}
		sort(a + 1, a + n + 1);
		Try(1, 1);
		if(!ok) cout << "-1";
		cout << endl;
	}
	return 0;
}
		
