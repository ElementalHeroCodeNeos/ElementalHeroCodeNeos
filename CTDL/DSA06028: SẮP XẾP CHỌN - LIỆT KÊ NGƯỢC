#include <bits/stdc++.h>

using namespace std;

vector<vector<int>> res;
int cnt = 0;

void selectionSort(int a[], int n){
	for(int i=0; i<n-1; i++){
		int min_pos = i;
		for(int j=i+1; j<n; j++){
			if(a[j] < a[min_pos]){
				min_pos = j;
			}
		}
		swap(a[i], a[min_pos]);
		vector<int> tmp(a, a + n);
		res.push_back(tmp);
		cnt++;
	}	
}

int main(){
	int n; cin >> n;
	int a[n];
	for(int &x : a) cin >> x;
	selectionSort(a, n);
	while(cnt){
		cout << "Buoc " << cnt << ": ";
		for(int x : res[cnt - 1]){
			cout << x << " ";
		}
		cout << endl;
		cnt--;
	}
	return 0;
}
