#include <bits/stdc++.h>

using namespace std;

using ll = long long;
	
int main(){
	int tc; cin >> tc;
	while(tc--){
		int n, k; cin >> n >> k;
		int a[n];
		for(int &x : a) cin >> x;
		sort(a, a + n);
		ll cnt = 0;
		for(int i=0; i<n-1; i++){
			auto it = lower_bound(a + i + 1, a + n, a[i] + k);
			cnt += (it - a) - (i + 1);
		}
		cout << cnt << endl;
	}
	return 0;
}
