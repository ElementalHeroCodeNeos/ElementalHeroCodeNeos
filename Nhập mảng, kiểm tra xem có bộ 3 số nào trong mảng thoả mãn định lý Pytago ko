#include <stdio.h>
#include <math.h>
#define lld long long


void sort(lld a[100], int n){
	for(int i=0; i<n; i++){
		for(int j=i; j<n; j++){
			if(a[j]<a[i]){
				int temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}	
	}
}


int check(lld a[100], int n){
	for(int i=n-1; i>=2; i--){
		int l=0, r=i-1;
		while(l<r){
			if(a[l]+a[r]==a[i])
				return 1;
			else if(a[l]+a[r]<a[i]){
				l++;
			}
			else{
				r--;
			}
		}
	}
	return 0;
}

int main(){
	int t;
	scanf("%d", &t);
	while(t--){
		int n;
		scanf("%d", &n);
		lld a[100];
		for(int i=0; i<n; i++){
			scanf("%d", &a[i]);
			a[i] *= a[i];
		}
		
		if(check(a,n)==1){
			printf("YES\n");
		}
		else{
			printf("NO\n");
		}
	}
}
		
		
