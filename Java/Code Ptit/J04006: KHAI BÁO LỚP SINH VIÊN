import java.util.Scanner;

class SinhVien {
    private String msv, ten, lop, ns;
    private double gpa;
    public SinhVien(String msv, String ten, String lop, String ns, double gpa){
        this.msv = msv;
        this.ten = ten;
        this.lop = lop;
        this.ns = ns;
        this.gpa = gpa;
    }
    public void chuanHoaNs(){
        StringBuilder sb = new StringBuilder(this.ns);
        if(sb.charAt(1) == '/') sb.insert(0, "0");
        if(sb.charAt(4) == '/') sb.insert(3, "0");
        this.ns = sb.toString();
    }
    public String toString(){
        return this.msv + " " + this.ten + " " + this.lop + " " + this.ns + " " + String.format("%.2f", this.gpa);
    }
}

public class ptit {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        SinhVien x = new SinhVien("B20DCCN001", sc.nextLine(), sc.nextLine(), sc.nextLine(), sc.nextDouble());
        x.chuanHoaNs();
        System.out.println(x);
    }
}
