import java.util.Scanner;

public class test { 
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int tc = sc.nextInt();
        for(int t=1; t<=tc; t++){
            int n = sc.nextInt();
            double h = sc.nextDouble();
            for(int i=1; i<=n-1; i++){
                // 2 tam giác đồng dạng: tỉ số diện tích = bình phương tỉ số đồng dạng = bình phương tỉ số chiều cao
                double dis = Math.sqrt((double)i / n) * h;  
                System.out.printf("%.6f ", dis);
            }
            System.out.println("");
        }
    }
}
