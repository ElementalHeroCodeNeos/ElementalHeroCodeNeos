import java.util.Scanner;

class NhanVien {
    private String ma, ten, chucVu;
    private int luongNgay, soNgay;

    public NhanVien(int ma, String ten, int luongNgay, int soNgay, String chucVu) {
        this.ma = "NV" + String.format("%02d", ma);
        this.ten = ten;
        this.chucVu = chucVu;
        this.luongNgay = luongNgay;
        this.soNgay = soNgay;
    }
    
    public int getLuongThang(){
        return luongNgay * soNgay;
    }
    
    public int getThuong(){
        if(soNgay >= 25) return this.getLuongThang() * 20 / 100;
        if(soNgay >= 22) return this.getLuongThang() * 10 / 100;
        return 0;
    }
    
    public int getPhuCap(){
        if(chucVu.equals("GD")) return 250000;
        if(chucVu.equals("PGD")) return 200000;
        if(chucVu.equals("TP")) return 180000;
        return 150000;
    }
    
    public int getThucLinh(){
        return this.getLuongThang() + this.getThuong() + this.getPhuCap();
    }
    
    public String toString(){
        return ma + " " + ten + " " + this.getLuongThang() + " " + this.getThuong() + " "+ this.getPhuCap()
        + " " + this.getThucLinh();
    }
}

public class J05038 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        NhanVien[] a = new NhanVien[n];
        sc.nextLine();
        for(int i=0; i<n; i++){
            String ten = sc.nextLine();
            int luongNgay = sc.nextInt(), soNgay = sc.nextInt();
            sc.nextLine();
            String chucVu = sc.nextLine();
            a[i] = new NhanVien(i + 1, ten, luongNgay, soNgay, chucVu);
        }
        int sum = 0;
        for(NhanVien x : a){
            sum += x.getThucLinh();
            System.out.println(x);
        }
        System.out.println("Tong chi phi tien luong: " + sum);
    }
}
