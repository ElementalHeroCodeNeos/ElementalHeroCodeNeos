import java.util.Scanner;
// Chú ý: Bài này đề lỏ, bảo dựa vào ký tự thứ 2 và 3 trong MSV nhưng phải dùng mã lớp mới đúng!
class SinhVien {
    private String msv, ten, lop, email;

    public SinhVien(String msv, String ten, String lop, String email) {
        this.msv = msv;
        this.ten = ten;
        this.lop = lop;
        this.email = email;
    }

    public String getMsv() {
        return msv;
    }

    public String getLop() {
        return lop;
    }

    public String toString() {
        return msv + " " + ten + " " + lop + " " + email;
    }
}

public class main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        SinhVien[] a = new SinhVien[n];
        sc.nextLine();
        for(int i=0; i<n; i++){
            a[i] = new SinhVien(sc.nextLine(), sc.nextLine(), sc.nextLine(), sc.nextLine());
        }
        int Q = sc.nextInt();
        for(int t=0; t<Q; t++){
            String khoa = sc.next();
            System.out.println("DANH SACH SINH VIEN KHOA " + khoa + ":");
            for(SinhVien x : a){
                if(x.getLop().substring(1, 3).equals(khoa.substring(2))){
                    System.out.println(x);
                }
            }
        }
    }
}
