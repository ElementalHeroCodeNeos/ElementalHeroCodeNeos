#include <bits/stdc++.h>

using namespace std;

int Miss_number(int a[], int n){
	int sum = (1 + n) * n / 2;	// Tông cua n sô tu nhiên dau tiên
	for(int i=0; i<n-1; i++){	
		sum -= a[i];	
	}
	return sum;
}

int main(){
	int tc; cin >> tc;
	while(tc--){
		int n; cin >> n;
		int a[n-1];
		for(int i=0; i<n-1; i++) cin >> a[i];	
		cout << Miss_number(a, n) << endl;
	}
	return 0;
}
