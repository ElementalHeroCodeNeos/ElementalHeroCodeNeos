#include <stdio.h>
#include <string.h>
#include <ctype.h>
#include <stdlib.h>

int main(){
	int t;
	scanf("%d",&t);
	while(t--){
		char c[10001];
		scanf("%s",c);
	
		int res=0,sum=0;
		for(int i=0;i<strlen(c);i++){
			if(isdigit(c[i])){                              
				res = res*10 + c[i]-'0';
			}
			else{                                         // Nếu kí tự k phải chữ số thì tức là số lưu trong res đã đc hoàn thiện, lấy sum+=res(nhưng vì ktra vậy thì phải xét trường hợp kí tự cuối)
				sum+=res;
				res=0;
			}
		}
		if(isdigit(c[strlen(c)-1])){                    // Kiểm tra trong trường hợp kí tự cuối là chứ số(digit)
			sum += res;
		}
		printf("%d\n",sum);
	}
}
	
	
