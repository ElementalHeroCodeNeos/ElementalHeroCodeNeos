#include <bits/stdc++.h>

using namespace std;

int n, k, a[21];
bool ok;

void sinh(){	// Câu hình dau tiên
	for(int i=1; i<=k; i++){
		a[i] = i;
	}
}

void next(){
	int i = k;
	while(i >= 1 && a[i] == n - k + i){		// n - k + i là max cua phan tu a[i]
		i--;
	}
	if(i == 0) ok = 0;	// Da dên câu hình cuôi cùng
	else{
		a[i]++;
		for(int j=i+1; j<=n; j++){	// Các phân tu sau vi trí i là day tang liên tiêp
			a[j] = a[j-1] + 1;
		}
	}
}

int main(){
	int tc; cin >> tc;
	while(tc--){
		cin >> n >> k;
		ok = 1;
		sinh();
		while(ok){
			for(int i=1; i<=k; i++){
				cout << a[i];
			}
			cout << " ";
			next();
		}
		cout << endl;
	}
	return 0;
}
