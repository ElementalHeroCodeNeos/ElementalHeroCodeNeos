#include <bits/stdc++.h>

using namespace std;

int main(){
	int tc; cin >> tc;
	while(tc--){
		char kitu = ' ';
		int n=0, c=0, l=0;
		while(kitu!='\n'){
			int x; cin >> x;
			n++;
			if(x%2==0) c++;
			else l++;
			kitu = getchar();
		}
		if(n%2==0 && c>l || n%2==1 && l>c){
			cout << "YES\n";
		}
		else cout << "NO\n";
	}
	return 0;
}
		
		
		
		
		
		
		
