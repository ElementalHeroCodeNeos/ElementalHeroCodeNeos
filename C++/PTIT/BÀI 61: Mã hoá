#include <bits/stdc++.h>

using namespace std;

int main(){
	int tc; cin >> tc;
	while(tc--){
		string s; cin >> s;
		map<char, int> mp;
		for(int i=0; i<s.size(); i++){
			mp[s[i]]++;
		}
		for(int i=0; i<s.size(); i++){
			if(s[i] == s[i+1]) continue;
			cout << s[i] << mp[s[i]];
		}
		cout << endl;
	}
	return 0;
}
		
		
		
		
