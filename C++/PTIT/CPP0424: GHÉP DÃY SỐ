#include <bits/stdc++.h>

using namespace std;

int main(){
	int tc; cin >> tc;
	while(tc--){
		int k, n; cin >> k >> n;
		multiset<int> s;
		while(k--){
			for(int i=0; i<n; i++){
				int x; cin >> x;
				s.insert(x);
			}
		}
		for(int x : s){
			cout << x << " ";
		}
		cout << endl;
	}
	return 0;
}
