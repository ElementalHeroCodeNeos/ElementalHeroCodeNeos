#include <bits/stdc++.h>

using namespace std;
// Do d <= n 
int main(){
	int tc; cin >> tc;
	while(tc--){
		int n, d; cin >> n >> d;
		int a[n];
		for(int i=0; i<n; i++) cin >> a[i];
		for(int i=d; i<n; i++) cout << a[i] << " ";
		for(int i=0; i<d; i++) cout << a[i] << " ";
		cout << endl;
	}
	return 0;
}
