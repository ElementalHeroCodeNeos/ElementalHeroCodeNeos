#include <bits/stdc++.h>

using namespace std;

void solve(int n){
	for(int i=2; i<=sqrt(n); i++){
		if(n%i == 0){
			int cnt = 0;
			while(n%i == 0){
				n /= i;
				cnt++;
			}
			cout << i << " " << cnt << " "; 
		}
	}
	if(n != 1){
		cout << n << " 1";
	}
	cout << endl;	
}


int main(){
	int tc; cin >> tc;
	while(tc--){
		int n; cin >> n;
		solve(n);
	}
	return 0;
}
