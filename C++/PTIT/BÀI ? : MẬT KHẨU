#include <bits/stdc++.h>

using namespace std;

bool cmp(string a, string b){
	return a.size() > b.size();
}
		
int main(){
	int n; cin >> n;
	string a[n];
	int ans = 0;
	for(int i=0; i<n; i++) cin >> a[i];
	sort(a, a + n, cmp);
	for(int i=0; i<n; i++){
		for(int j=i+1; j<n; j++){
			if(a[i] == a[j]) ans += 2;
			else if(a[i].find(a[j]) != -1) ans++;	
		}
	}
	cout << ans << endl;
}
	
