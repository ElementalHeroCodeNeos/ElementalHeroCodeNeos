#include <bits/stdc++.h>

using namespace std;

using ll = long long;

ll solve(ll n){
	ll res = 0;
	for(int i=2; i<=sqrt(n); i++){
		while(n%i == 0){
			res = i;
			n /= i;
		}
	}
	if(n != 1) res = n;
	return res;
}

int main(){
	int tc; cin >> tc;
	while(tc--){
		ll n; cin >> n;
		cout << solve(n) << endl;
	}
	return 0;
}
