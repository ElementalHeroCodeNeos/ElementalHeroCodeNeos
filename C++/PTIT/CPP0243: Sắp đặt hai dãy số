#include <bits/stdc++.h> 

using namespace std; 

int main(){
	int tc; cin >> tc;
	while(tc--){
		int n, m; cin >> n >> m;
		int a[n], b[m];
		map<int, int> mp;
		vector<int> v;
		for(int i=0; i<n; i++){
			cin >> a[i];
			mp[a[i]]++;
		}
		sort(a, a + n);
		for(int i=0; i<m; i++) cin >> b[i];
		for(int i=0; i<m; i++){
			int cnt = mp[b[i]];
			for(int j=0; j<cnt; j++){
				v.push_back(b[i]);
				mp[b[i]]--;
			}
		}
		for(int i=0; i<n; i++){
			if(mp[a[i]] != 0) v.push_back(a[i]);
		}
		for(int x : v) cout << x << " ";
		cout << endl;
	}
	return 0;
}
