#include <bits/stdc++.h>

using namespace std;
// Mang công dôn (prefix sum)
int main(){
	int tc; cin >> tc;
	while(tc--){
		int n, Q; cin >> n >> Q;
		int a[n];
		for(int i=0; i<n; i++) cin >> a[i];
		int sum[n];	// sum[i] là tông các phan tu tu a[0] den a[i]
		for(int i=0; i<n; i++){
			if(i == 0) sum[i] = a[i];
			else sum[i] = sum[i-1] + a[i];
		}
		int l, r; 
		while(Q--){
			cin >> l >> r;
			l--; r--;
			if(l == 0) cout << sum[r] << endl; 
			else cout << sum[r] - sum[l-1] << endl;
		}
	}
	return 0;
}
