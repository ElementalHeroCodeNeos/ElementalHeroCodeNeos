#include <bits/stdc++.h>

using namespace std;

int main(){
	int tc; cin >> tc;
	while(tc--){
		int n, m; cin >> n >> m;
		int a[n], b[m];
		multiset<int> s;
		for(int i=0; i<n; i++){
			cin >> a[i];
			s.insert(a[i]);
		}
		for(int j=0; j<m; j++){
			cin >>  b[j];
			s.insert(b[j]);
		}
		for(int x : s){
			cout << x << " ";
		}
		cout << endl;
	}
	return 0;
}
