#include <bits/stdc++.h>

using namespace std;

int GCD(int a, int b){
	if(b==0) return a;
	return (b, a%b);
}

int main(){
	int tc; cin >> tc;
	while(tc--){
		int n; cin >> n;
		int a[n];
		for(int i=0; i<n; i++) cin >> a[i];
		int res = 1;
		for(int i=0; i<n; i++){
			res = GCD(res, a[i]);        Tính res =  ước chung lớn nhất của tất cả phần tử trong mảng ---> Các số x sẽ là các ước của res
		}
		int ans = 1;
		for(int i=2; i<=sqrt(res); i++){
			if(res % i == 0){
				ans++;
				if(i != res/i) ans++;
			}
		}
		cout << ans << endl;
	}
	return 0;
}
		
		
		
