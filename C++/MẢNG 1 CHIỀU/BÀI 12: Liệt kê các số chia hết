#include <bits/stdc++.h>

using namespace std;

map<int, int> mp;

void solve(int n){
	for(int i=1; i<=sqrt(n); i++){
		if(n%i==0){
			mp[i]++;
			if(i != n/i) mp[n/i]++;
		}
	}
}

int main(){
	int tc; cin >> tc;
	while(tc--){
		int n; cin >> n;
		int a[n];
		for(int i=0; i<n; i++){
			cin >> a[i];
			solve(a[i]);
		}
		int cnt = 0;
		for(auto it : mp){
			if(it.second == n) cnt++;			
		}
		cout << cnt - 1 << endl;
		mp.clear();
	}
	return 0;
}
			
			
		
