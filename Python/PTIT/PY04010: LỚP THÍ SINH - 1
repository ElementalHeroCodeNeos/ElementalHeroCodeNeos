import math

class ThiSinh:
    def __init__(self, ten, ns, d1, d2, d3):
        self.__ten = ten
        self.__ns = ns
        self.__d1 = d1
        self.__d2 = d2
        self.__d3 = d3
    def get_tongdiem(self):
        return self.__d1 + self.__d2 + self.__d3
    def __str__(self):
        return f'{self.__ten} {self.__ns} {self.get_tongdiem():.1f}'

if __name__ == '__main__':
    t = ThiSinh(input(), input(), float(input()), float(input()), float(input()))
    print(t)
