import math

if __name__ == '__main__':
    tc = int(input())
    for case in range(tc):
        n, d = map(int, input().split())
        a = list(map(int, input().split()))
        d %= n
        for i in range(d, n): 
            print(a[i], end = ' ')
        for i in range(0, d):
            print(a[i], end = ' ')
        print()
