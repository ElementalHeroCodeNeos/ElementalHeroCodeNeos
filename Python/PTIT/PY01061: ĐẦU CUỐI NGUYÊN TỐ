import math

def nto(n):
    for i in range(2, math.isqrt(n) + 1):
        if n % i == 0:
            return False
    return n > 1

if __name__ == '__main__':   
    tc = int(input())
    for case in range(tc):
        s = input()
        if nto(int(s[:3])) and nto(int(s[len(s) - 3 : len(s)])):
            print('YES')
        else:
            print('NO')
